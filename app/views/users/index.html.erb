<h1>Profiles</h1>

  <% if current_user %>
  <p> You are logged in as <%= link_to current_user[:name], user_path(current_user) %> </p>
  <% end %>

  <%= link_to "log-out", log_out_path %>
<p>
  <%= link_to "Register New Account" %>
</p>
<p>
  <%= link_to "Login to an Existing Account", log_in_path %>
</p>
<ul>
  <% if @users.nil? == false %>
  <% @users.each do |user| %>
  <li><%= link_to user.name, user_path(user) %></li>
  <% end %>
  <% end %>
</ul>
